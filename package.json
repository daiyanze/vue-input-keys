{
  "name": "vue-keymap",
  "version": "0.0.1",
  "description": "A Vuejs 2.x plugin for binding keys to components",
  "main": "dist/index.js",
  "types": "types/index.d.ts",
  "scripts": {
    "dev": "parcel docs/src/index.html --out-dir ./docs",
    "build": "yarn run build:js && yarn run build:docs",
    "build:js": "tsc -p ./tsconfig.json",
    "build:docs": "rimraf ./docs/main* ./docs/index.html && parcel build docs/src/index.html --out-dir ./docs",
    "clean": "rimraf ./dist",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/daiyanze/vue-keymap.git"
  },
  "keywords": [
    "vue",
    "vue-keymap",
    "keybindings",
    "shortcut"
  ],
  "author": "daiyanze <ginoalex8964@yahoo.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/daiyanze/vue-keymap/issues"
  },
  "homepage": "https://github.com/daiyanze/vue-keymap#readme",
  "devDependencies": {
    "@vue/component-compiler-utils": "^2.1.0",
    "parcel-bundler": "^1.9.7",
    "rimraf": "^2.6.2",
    "tslint": "^5.11.0",
    "typescript": "^2.9.2",
    "vue": "^2.5.16",
    "vue-hot-reload-api": "^2.3.0",
    "vue-template-compiler": "^2.5.16",
    "yarn": "^1.9.2"
  }
}
